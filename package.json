{
    "name": "angular-video-player",
    "homepage": "https://un2mercey.github.io/angular-video-player/",
    "version": "0.0.0",
    "scripts": {
        "i": "npm i --no-fund",
        "ng": "ng",
        "prestart": "npm run lint-fix",
        "start": "ng serve",
        "build": "ng build",
        "deploy": "ng deploy --base-href=/angular-video-player/",
        "watch": "ng build --watch --configuration=development",
        "test": "ng test",
        "lint": "ng lint",
        "lint-fix": "npm run lint --fix",
        "audit-fix": "npm audit fix",
        "audit-fix-l": "npm audit fix --legacy-peer-deps",
        "audit-fix-f": "npm audit fix --force",
        "check-updates": "npx npm-check-updates -u"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~12.2.1",
        "@angular/cdk": "^12.2.1",
        "@angular/common": "~12.2.1",
        "@angular/compiler": "~12.2.1",
        "@angular/core": "~12.2.1",
        "@angular/forms": "~12.2.1",
        "@angular/material": "^12.2.1",
        "@angular/platform-browser": "~12.2.1",
        "@angular/platform-browser-dynamic": "~12.2.1",
        "@angular/router": "~12.2.1",
        "rxjs": "~7.3.0",
        "tslib": "^2.3.1",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~12.2.1",
        "@angular-eslint/builder": "12.3.1",
        "@angular-eslint/eslint-plugin": "12.3.1",
        "@angular-eslint/eslint-plugin-template": "12.3.1",
        "@angular-eslint/schematics": "12.3.1",
        "@angular-eslint/template-parser": "12.3.1",
        "@angular/cli": "~12.2.1",
        "@angular/compiler-cli": "~12.2.1",
        "@types/jasmine": "~3.8.2",
        "@types/node": "^16.6.1",
        "@typescript-eslint/eslint-plugin": "4.29.1",
        "@typescript-eslint/parser": "4.29.1",
        "angular-cli-ghpages": "^1.0.0",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-typescript": "^12.3.1",
        "eslint-plugin-import": "^2.24.0",
        "jasmine-core": "~3.8.0",
        "karma": "~6.3.4",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.1",
        "karma-jasmine-html-reporter": "~1.7.0",
        "typescript": "~4.3.5"
    }
}
